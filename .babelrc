{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "node": 10
            }
        }]
    ],
    "plugins": [
        "@babel/plugin-proposal-export-default-from"
    ],
    "env": {
        "development": {
            "sourceMaps": "inline",
            "plugins": ["source-map-support"]
        }
    },
    "overrides": [{
        "test": ["/underscore.js/", "/websocket.js/"],
        "sourceType": "script"
    },{
        "test": "**/src/ox_modules/**/*.js",
        "comments": true
    }],
    "ignore": [
        "**/src/lib/logger.js"
    ],
    "comments": false
}
