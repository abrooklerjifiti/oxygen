{
  "name": "oxygen-cli",
  "version": "1.0.3",
  "description": "Automation framework for Web & Mobile applications",
  "main": "./build/index.js",
  "bin": {
    "oxygen": "./build/lib/cli.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/oxygenhq/oxygen.git"
  },
  "keywords": [
    "selenium",
    "appium",
    "webdriver",
    "testing",
    "automation",
    "qa"
  ],
  "author": {
    "name": "CloudBeat Limited",
    "email": "info@oxygenhq.org",
    "url": "http://oxygenhq.org"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/oxygenhq/oxygen/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/node": "7.5.5",
    "@babel/runtime": "7.5.5",
    "babel-plugin-source-map-support": "2.1.1",
    "cross-env": "6.0.3",
    "doctrine": "3.0.0",
    "eslint": "6.8.0",
    "husky": "3.1.0",
    "npm-run-all": "4.1.5",
    "source-map-support": "0.5.16",
    "md-reverse": "1.0.0"
  },
  "dependencies": {
    "@applitools/eyes-webdriverio": "5.9.11",
    "@babel/cli": "7.2.3",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-optional-catch-binding": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.5.5",
    "@babel/register": "7.5.5",
    "@lambdatest/node-rest-client": "1.0.0",
    "@wdio/cli": "5.18.6",
    "@wdio/devtools-service": "5.16.16",
    "@wdio/sync": "5.18.6",
    "async": "3.1.1",
    "chai": "4.2.0",
    "chrome-har": "0.11.2",
    "config": "3.2.5",
    "core-js": "3.6.4",
    "csv-parse": "4.8.5",
    "cucumber": "5.1.0",
    "deasync": "0.1.19",
    "decache": "4.5.1",
    "easyxml": "2.0.1",
    "ejs": "2.7.1",
    "fibers": "4.0.2",
    "glob": "7.1.6",
    "glob-to-regexp": "0.4.1",
    "globule": "1.3.0",
    "imap-simple": "4.3.0",
    "is-glob": "4.0.1",
    "junit-report-builder": "1.3.3",
    "lodash": "4.17.15",
    "minimist": "1.2.0",
    "mock-require": "3.0.3",
    "mockery": "2.1.0",
    "moment": "2.24.0",
    "ox-chrome-remote-interface": "0.28.1",
    "oxygen-logger": "0.4.0",
    "pdfreader": "1.0.4",
    "request": "2.88.0",
    "saucelabs": "3.0.1",
    "serialport": "8.0.7",
    "soap": "0.30.0",
    "testingbot-api": "1.0.7",
    "twilio": "3.39.4",
    "webdriverio": "5.18.6",
    "when": "3.7.8",
    "xlsx": "0.15.4"
  },
  "optionalDependencies": {
    "odbc": "1.4.6"
  },
  "scripts": {
    "build": "run-s clean compile",
    "clean": "rimraf ./build",
    "compile": "babel src/ -d build/ --config-file ./.babelrc --copy-files",
    "test": "run-s test:*",
    "test:lint": "eslint src test",
    "test:unit": "jest",
    "prepublishOnly": "npm run build && node tools/dos2unix src/lib/cli.js",
    "postpublish": "node tools/unix2dos src/lib/cli.js",
    "lint": "eslint \"src/**/*.js\" \"tools/**/*.js\"",
    "apidoc": "node tools/apidoc-wrap",
    "prod": "node ./build/lib/cli.js",
    "dev": "cross-env DEBUG=true NODE_ENV=development babel-node --config-file ./.babelrc ./src/lib/cli.js",
    "debug": "cross-env DEBUG=true babel-node --debug=5858 --presets @babel/preset-env ./src/lib/cli.js"
  }
}
